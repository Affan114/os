#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <sys/types.h>
#include <sys/stat.h>
#include <fcntl.h>
#include <unistd.h>
#include <sys/mman.h>
int main ()
{
	fork();
	fork();
	fork();
	printf("Hello World\n");
	return 0;
}
output:
Hello World
Hello World
Hello World
Hello World
Hello World
Hello World
Hello World
Hello World
